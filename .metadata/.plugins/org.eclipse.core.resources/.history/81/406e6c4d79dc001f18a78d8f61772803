package AllPrograms;

public class ArmstrongNumber 
{
	public static void main(String[] args) 
	{
		int number=1634;
		int sum=0;
		int rem;
		for(int i=number;i>0;i=i/10)
		{
			rem=i%10;
			sum=sum+(rem*rem*rem*rem);
			
		}
		
		if(number==sum)
		{
			System.out.println("Number is armstrong");
		}
		else
		{
			System.out.println("Number is not armstrong");
		}
	}

}
