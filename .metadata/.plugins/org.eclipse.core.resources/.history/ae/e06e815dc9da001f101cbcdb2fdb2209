package AllPrograms;

public class RevereseNumber 
{
	public static void main(String[] args) {
		int num=2345;
		int reverse=0;
		int temp=0;
		
		System.out.println("Number :"+num);
		
		while(num>0)
		{
			temp=num%10;
			reverse=reverse*10+temp;
			num=num/10;
		}
		
		System.out.println("Reverse Number :"+reverse);
	}

}
