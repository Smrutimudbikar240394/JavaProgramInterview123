package StringProgram;

public class ReverseEvenIndexWordInStatement 
{
	public static void main(String[] args) 
	{
		String str="My name is ABC";
		String[] ar=str.split(" ");
		
		for(int i=0;i<ar.length;i++)
		{
			String org=ar[i];
			
			if(i%2==0)
			{
				String rev=ReverseString(org);
			}
		}
		
	}

}
